<%- include partials/header -%>

<h1>Home</h1>

<p> <%= homeStartingContent %> </p>


<% posts.forEach(function(post) { %>
  <div class="postHolder">
    <h3> <%= post.title %> </h3>
    <p> <%= post.body.substring(0, 100) + " ..."; %> </p>
    <a href="/posts/<%=post.title%>" > Read More.</a>
  </div>
<% }); %>

<%- include partials/footer -%>
